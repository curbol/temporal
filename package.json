{
  "name": "temporal-keyboard",
  "version": "1.0.0",
  "description": "Temporal keyboard ergogen configuration",
  "scripts": {
    "gen": "cd ergogen && ergogen . -o output",
    "convert": "cd ergogen/output/cases && for f in *.jscad; do npx @jscad/cli@1 \"$f\" -of stla; done"
  },
  "devDependencies": {
    "ergogen": "^4.1.0",
    "@jscad/cli": "^1.10.0"
  }
}
